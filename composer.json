{
    "name": "maliboot/validation",
    "description": "A validation library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "validation"
    ],
    "support": {
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "MaliBoot\\Validation\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MaliBootTest\\Validation\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "MaliBoot\\Validation\\ConfigProvider"
        }
    }
}